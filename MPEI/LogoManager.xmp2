using MpeCore.Classes;
using MpeCore;
using System.IO;

public class Script
{
    public static void Main(PackageClass packageClass, ActionItem actionItem)
    {
      string oldDll = Path.Combine(MpeInstaller.TransformInRealPath("%Skin%"), @"Windows\LogosManager.dll");

    try
    {
      if (File.Exists(oldDll))
     {
        File.Delete(oldDll);
     }
    }
    catch {}
    DirectoryInfo skinroot = new DirectoryInfo(MpeInstaller.TransformInRealPath("%Skin%"));
    DirectoryInfo[] skins = skinroot.GetDirectories();
    foreach (DirectoryInfo skin in skins)
   {

    try
    {
    if(File.Exists(Path.Combine(skin.FullName, "LogosManager.xml")))
        {
         File.Delete(Path.Combine(skin.FullName, "LogosManager.xml"));
         }
     }
    catch {}
              
      }


        return;
    }
}
